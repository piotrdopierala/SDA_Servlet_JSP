<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>SimpleSDA</servlet-name>
    <servlet-class>sda.javapoz12.sandbox.SimpleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdvancedSDA</servlet-name>
    <servlet-class>sda.javapoz12.sandbox.AdvancedServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WebCache</servlet-name>
    <servlet-class>sda.javapoz12.sandbox.WebCacheServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>WebCache</servlet-name>
    <url-pattern>/web/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SimpleSDA</servlet-name>
    <url-pattern>/servlet/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AdvancedSDA</servlet-name>
    <url-pattern>/advanced/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AdvancedSDA</servlet-name>
    <url-pattern>/zaawansowany/*</url-pattern>
  </servlet-mapping>

</web-app>
